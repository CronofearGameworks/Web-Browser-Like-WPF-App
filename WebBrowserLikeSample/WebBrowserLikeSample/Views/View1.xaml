<UserControl x:Class="WebBrowserLikeSample.Views.View1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:prism="http://prismlibrary.com/"
             xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
             prism:ViewModelLocator.AutoWireViewModel="True"
             xmlns:local="clr-namespace:WebBrowserLikeSample.Views"
             md:TransitionAssist.DisableTransitions="True"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <md:DialogHost IsOpen="{Binding IsModalOpen}">
            <md:DialogHost.DialogContent>
                <md:Card Height="500" Width="500" >
                    <Button>Test</Button>
                </md:Card>
            </md:DialogHost.DialogContent>
        </md:DialogHost>
        <TextBlock>Hey</TextBlock>
    </Grid>
</UserControl>
